mongo
use tesisdb

 mr = db.runCommand({   "mapreduce" : "users",     "map" : function() {       for (var key in this) { emit(key, null); }     },     "reduce" : function(key, stuff) { return null; },     "out": "users" + "_keys"   })

db[mr.result].distinct("_id")

